{
  "require": {
    "firebase/php-jwt": "^6.11",
    "vlucas/phpdotenv": "^5.6",
    "phpmailer/phpmailer": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "AliveChMS\\Core\\": "core/",
      "AliveChMS\\Routes\\": "routes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AliveChMS\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  }
}
